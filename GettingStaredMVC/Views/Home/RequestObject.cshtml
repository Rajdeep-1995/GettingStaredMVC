
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RequestObject</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery-3.6.0.js"></script>
    <script src="~/Scripts/umd/popper.js"></script>
</head>
<body>
    <div>
        <nav class="navbar navbar-expand-sm bg-success navbar-dark ">
            <ul class="navbar nav">
                <li class="nav-item"><a class="nav-link text-light" href="/home/index">Home</a></li>
                <li class="nav-item"><a class="nav-link text-light" href="/home/products">Products</a></li>
                <li class="nav-item"><a class="nav-link text-light" href="/home/contact">Contact Us</a></li>
            </ul>
        </nav>
        <table class="table">
            <tr>
                <td>Url</td>
                <td>@ViewBag.Url</td>
            </tr>
            <tr>
                <td>PhysicalApplication Path</td>
                <td>@ViewBag.PhysicalAppliactionPath</td>
            </tr>
            <tr>
                <td>Path</td>
                <td>@ViewBag.Path</td>
            </tr>
            <tr>
                <td>Browser Type</td>
                <td>@ViewBag.BrowserType</td>
            </tr>
            <tr>
                <td>Query String</td>
                <td>@ViewBag.QueryString</td>
            </tr>
            <tr>
                <td>Headers</td>
                <td>@ViewBag.Header </td>
            </tr>
            <tr>
                <td>HTTP Method</td>
                <td>@ViewBag.HttpMehod</td>
            </tr>
        </table>
    </div>
</body>
</html>
